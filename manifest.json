{
  "manifest_version": 2,

  "name": "Keep It",
  "description": "Keep It button for Keep.com",
  "version": "0.9.3",
  "author": "Keep.com",
  "icons": {
    "128":"images/favicon_chrome.png"
  },

  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": [],
  "browser_action": {
    "default_icon": "images/favicon_chrome.png",
    "default_title": "Keep It"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+K",
        "mac": "Command+K"
      }
    }
  }
}