{
  "manifest_version": 2,

  "name": "Keep Shopping",
  "description": "Keep It button for Keep.com",
  "version": "1.0.0",
  "author": "Keep.com",
  "icons": {
    "128":"images/favicon_chrome.png",
    "32": "images/favicon_retina.png"
  },

  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_security_policy": "script-src 'self' https://keep.com; object-src 'self'",
  "permissions": [
    "https://keep.com/",
    "http://keep.com/",
    "<all_urls>",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/favicon_chrome.png",
    "default_title": "Keep Shopping"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+K",
        "mac": "Command+K"
      }
    }
  }
}